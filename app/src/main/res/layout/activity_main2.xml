<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity2"
    tools:layout_editor_absoluteX="-6dp"
    tools:layout_editor_absoluteY="-22dp">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="1dp"
        android:text="DINDING"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="38sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:text="Panjang"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/etPanjang"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etPanjang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:ems="8"
        android:hint="Masukkan Panjang"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintStart_toEndOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="148dp"
        android:layout_marginBottom="150dp"
        android:text="Tinggi"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Hitung"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etTinggi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:ems="7"
        android:hint="Masukkan Tinggi"
        android:inputType="number"
        app:layout_constraintStart_toStartOf="@+id/etPanjang"
        app:layout_constraintTop_toBottomOf="@+id/etPanjang" />

    <EditText
        android:id="@+id/etJendela"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:ems="4"
        android:hint="Jumlah"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/etJendelaLuas"
        app:layout_constraintStart_toStartOf="@+id/etTinggi" />

    <EditText
        android:id="@+id/etJendelaLuas"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:ems="3"
        android:hint="Luas"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/cbZakEmpat"
        app:layout_constraintStart_toEndOf="@+id/etJendela"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <EditText
        android:id="@+id/etPintu"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:ems="4"
        android:hint="Jumlah"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/etPintuLuas"
        app:layout_constraintStart_toStartOf="@+id/etJendela" />

    <CheckBox
        android:id="@+id/cbPintu"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:text="Pintu"
        app:layout_constraintBottom_toBottomOf="@+id/cbZakEmpat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cbZakLima" />

    <EditText
        android:id="@+id/etPintuLuas"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="81dp"
        android:layout_marginBottom="80dp"
        android:ems="3"
        android:hint="Luas"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintStart_toEndOf="@+id/etJendela"
        app:layout_constraintTop_toBottomOf="@+id/etPanjang" />

    <CheckBox
        android:id="@+id/cbJendela"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="7dp"
        android:text="Jendela"
        app:layout_constraintBottom_toBottomOf="@+id/etJendela"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etTinggi" />

    <Button
        android:id="@+id/btn_Hitung"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="278dp"
        android:text="HITUNG"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="1dp"
        android:text="Harga"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvHargabata"
        app:layout_constraintTop_toBottomOf="@+id/btn_Hitung" />

    <TextView
        android:id="@+id/tvBata"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargabata"
        app:layout_constraintTop_toTopOf="@+id/tvHargabata" />

    <TextView
        android:id="@+id/tvHargasemen"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/tvHargapasir" />

    <TextView
        android:id="@+id/tvSemenZak"
        android:layout_width="135dp"
        android:layout_height="70dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="1dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvSemen"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginEnd="130dp"
        android:text="Bata"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargabata"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvBata" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginEnd="130dp"
        android:text="Pasir"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargapasir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginEnd="130dp"
        android:text="Semen"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargasemen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/tvSemen"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargasemen"
        app:layout_constraintTop_toBottomOf="@+id/tvPasir" />

    <TextView
        android:id="@+id/tvHargabata"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="3dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/tvHargapasir"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="1dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/tvHargabata" />

    <TextView
        android:id="@+id/tvHargatotal"
        android:layout_width="145dp"
        android:layout_height="70dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tvSemen"
        app:layout_constraintTop_toBottomOf="@+id/tvHargasemen" />

    <TextView
        android:id="@+id/tvPasir"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:text="0"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvHargapasir"
        app:layout_constraintTop_toBottomOf="@+id/tvBata" />

    <CheckBox
        android:id="@+id/cbZakLima"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="1dp"
        android:text="50 Zak"
        app:layout_constraintBottom_toTopOf="@+id/cbZakEmpat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cbJendela" />

    <CheckBox
        android:id="@+id/cbZakEmpat"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="40 Zak"
        app:layout_constraintBottom_toBottomOf="@+id/etPintuLuas"
        app:layout_constraintStart_toStartOf="@+id/cbZakLima"
        app:layout_constraintTop_toTopOf="@+id/etPintuLuas" />


</androidx.constraintlayout.widget.ConstraintLayout>